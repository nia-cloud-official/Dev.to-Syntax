{
  "name": "Dev.to Syntax",
  "type": "light",
  "colors": {
    "editor.background": "#202020",
    "editor.foreground": "#d6d6d6",
    "editorLineNumber.foreground": "#6a737d",
    "editorCursor.foreground": "#d6d6d6",
    "editorIndentGuide.background1": "#30363d",
    "editorIndentGuide.activeBackground1": "#30353a"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "foreground": "#6a737d", "fontStyle": "italic" }
    },
    {
      "scope": ["keyword", "storage"],
      "settings": { "foreground": "#d73a49" }
    },
    {
      "scope": ["constant", "variable.language"],
      "settings": { "foreground": "#005cc5" }
    },
    {
      "scope": ["string", "constant.character.escape"],
      "settings": { "foreground": "#ffffff" }
    },
    {
      "scope": ["support.function", "entity.name.function"],
      "settings": { "foreground": "#ac88f0" }
    },
    {
      "scope": ["variable", "meta.definition.variable.name"],
      "settings": { "foreground": "#c7c5c4" }
    },
    {
      "scope": ["punctuation.separator", "punctuation.terminator"],
      "settings": { "foreground": "#586069" }
    }
  ]
}
